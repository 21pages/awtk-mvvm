{
  "name":"book",
  "desc":"book store",
  "collection":true,
  "cmp":"  return strcmp(a->name, b->name);",
  "props": [
    {
      "name":"name",
      "type":"char*",
      "init_value":"str_gen_random(\"book %d\")",
      "desc": "book name"
    },
    {
      "name":"stock",
      "type":"uint32_t",
      "init_value":"random()%100",
      "desc": "stock"
    }
  ],
  "cmds": [
    {
      "name":"sale",
      "canExec":"return book->stock > 0;",
      "impl":"book->stock--;",
      "desc":"sale a book"
    }
  ]
}
