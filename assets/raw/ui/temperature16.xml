<window v-model="temperature">
  <label x="center" y="middle" w="50%" h="40" v-data:text="{value}"/>
  <buzzer name="buzzer" visible="false" v-data:on="{$value > 50}" freq="3000" volume="60"/>
  <temperature_sensor name="sensor0" visible="false" v-data:value="{value, Mode=OneWayToModel}" sample_interval="3000"/>
</window>
